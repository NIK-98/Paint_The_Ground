; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Paint The Ground"
config/version="1.3.7"
run/main_scene="res://sceens/main.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://assets/icon/ptg.png"
config/icon="res://assets/icon/ptg.png"
boot_splash/minimum_display_time=3000
license/type="COPYRIGHT"
license/print_engine_license_in_console=true
license/project_name="Paint The Ground"
license/year=2025

[autoload]

Global="*res://scripts/Global.gd"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
LicenseMe="*res://addons/license_me/singleton.gd"

[debug]

gdscript/warnings/enable=false
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_signal=0
gdscript/warnings/untyped_declaration=2
gdscript/warnings/inferred_declaration=2
gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=800
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"
window/handheld/orientation=4
window/vsync/vsync_mode=0

[editor]

movie_writer/disable_vsync=true

[editor_plugins]

enabled=PackedStringArray("res://addons/better-terrain/plugin.cfg", "res://addons/license_me/plugin.cfg")

[file_customization]

folder_colors={
"res://": "green",
"res://assets/": "orange",
"res://sceens/": "red",
"res://scripts/": "yellow"
}

[filesystem]

import/blender/enabled=false

[global_group]

player=""
npc=""

